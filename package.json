{
  "name": "jenkins-node",
  "version": "0.0.0",
  "description": "sample project to run through jenkins",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/babel-cli/bin/babel server --out-dir bin/server",
    "clean-package": "node ./.config/clean-package",
    "copy-files": "node ./.config/build",
    "start": "node ./server",
    "test": "mocha",
    "watch": "nodemon ./.config/index --watch ./server"
  },
  "author": "Seth Lowie <seth.lowie@gmail.com> (https://github.com/logiXbomb)",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "copy-files": "^0.1.0",
    "copyfiles": "^0.2.1"
  }
}
